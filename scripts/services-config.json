      "name": "api-gateway",
      "displayName": "API Gateway",
      "type": "gateway",
      "port": 3001,
      "path": "apps/gateway",
      "buildCommand": "cd apps/gateway && npm install && npm run build",
      "startCommand": "cd apps/gateway && npm run start:prod",
      "healthCheckPath": "/health",
      "required": true,
      "deploymentPriority": 1,
      "dependencies": [],
      "infrastructureDependencies": ["redis"],
      "environmentVariables": [
        {
          "name": "DEPLOYMENT_PLATFORM",
          "required": false,
          "description": "Deployment Platform (railway, kubernetes, local)",
          "default": "railway"
        },
        {
          "name": "NODE_ENV",
          "required": false,
          "description": "Node.js Environment",
          "default": "production"
        },
        {
          "name": "JWT_SECRET",
          "required": true,
          "description": "Secret für JWT-Token-Generierung"
        },
        {
          "name": "CORS_ORIGIN",
          "required": false,
          "description": "CORS Origin (komma-separiert)",
          "default": "*"
        }
      ],
      "documented": true
